{
  "name": "@doc.ai/neuron-app",
  "version": "0.0.0-development",
  "description": "Neuron app framework",
  "main": "lib/index.js",
  "repository": "git@github.com:doc-ai/node-neuron-app.git",
  "license": "Apache-2.0",
  "author": "doc.ai Team <dev@doc.ai>",
  "contributors": [
    {
      "name": "node-neuron-app contributors",
      "url": "https://github.com/doc-ai/node-neuron-app/graphs/contributors"
    }
  ],
  "directories": [
    "lib/"
  ],
  "bin": {
    "neuron-app": "./bin/neuron-app.js"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "npm run clean && babel src -d lib",
    "watch": "npm run clean && babel src -w -d lib",
    "test": "echo 'No tests yet'",
    "lint": "mkdir -p reports && eslint -f junit . > reports/eslint.xml",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote --trailing-comma all",
      "eslint . --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.42",
    "@doc.ai/eslint-config-neuron": "^1.0.0",
    "async-exit-hook": "^2.0.1",
    "axios": "^0.18.0",
    "chalk": "^2.3.2",
    "concurrently": "^3.5.1",
    "consul": "^0.30.0",
    "create-error": "^0.3.1",
    "cross-spawn": "^6.0.5",
    "debug": "^3.1.0",
    "detect-port-alt": "^1.1.3",
    "dotenv": "^5.0.1",
    "ejs": "^2.5.7",
    "fs-extra": "^5.0.0",
    "http-errors": "^1.6.2",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-logger": "^3.1.0",
    "koa-router": "^7.3.0",
    "mime-types": "^2.1.17",
    "nodemon": "^1.17.2",
    "npm": "^5.7.1",
    "pm2": "^2.10.1",
    "rc": "^1.2.2",
    "semver": "^5.4.1",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.42",
    "@babel/core": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "@babel/preset-flow": "^7.0.0-beta.42",
    "@babel/preset-stage-0": "^7.0.0-beta.42",
    "cz-conventional-changelog": "^2.1.0",
    "flow-bin": "^0.68.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "prettier": "^1.7.4",
    "semantic-release": "^15.1.4"
  }
}
